<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- These beans are used by the InvokeURLAction. They define the
		 necessary verb support. -->
	<bean id="GET" class="com.eftps.http.invokers.GetMethodInvoker" />
	<bean id="POST" class="com.eftps.http.invokers.PostMethodInvoker" />
	<bean id="PUT" class="com.eftps.http.invokers.PutMethodInvoker" />
	<bean id="DELETE" class="com.eftps.http.invokers.DeleteMethodInvoker" />
</beans>
